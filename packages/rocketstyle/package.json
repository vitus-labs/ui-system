{
  "name": "@vitus-labs/rocketstyle",
  "version": "0.3.0-experimental.1",
  "license": "MIT",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "maintainers": [
    "Vit Bokisch <vit@bokisch.cz>"
  ],
  "homepage": "https://vitus-labs.github.io/rocketstyle/",
  "description": "Rocketstyle is ultra powerful and extensible styling system for building React components blazingly fast, easily and make them easily extensible and reusable.",
  "keywords": [
    "rocketstyle",
    "style",
    "styled-components",
    "css",
    "flexible",
    "extensible",
    "powerful",
    "styling",
    "styled",
    "system",
    "react",
    "reusability",
    "extensible"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vitus-labs/rocketstyle"
  },
  "bugs": {
    "url": "https://github.com/vitus-labs/rocketstyle/issues"
  },
  "main": "dist/vitus-labs-rocketstyle.js",
  "umd:main": "dist/vitus-labs-rocketstyle.umd.js",
  "module": "dist/vitus-labs-rocketstyle.module.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf lib && babel src -d lib --root-mode upward && cpr lib dist --delete-first && yarn rollup",
    "storybook": "start-storybook -p 6006 -c tools/storybook",
    "storybook:build": "build-storybook -c tools/storybook -o docs/storybook",
    "lint": "yarn lint:js",
    "lint:js": "eslint src",
    "lint:ts": "tslint -p ./tsconfig.json \"./src/**/*.ts*\"",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "cover": "coveralls < .coverage/lcov.info",
    "rollup": "rollup -c",
    "pkgs": "rm -rf node_modules/ package-lock.json yarn.lock && yarn",
    "prettier": "prettier \"./*.+(js|json|md)\" \"./src/**/*.*\" --write",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.1.0"
  },
  "peerDependencies": {
    "@vitus-labs/core": ">= 0.1 <1",
    "react": ">= 16.7"
  },
  "devDependencies": {
    "@internal/rollup": "*",
    "@vitus-labs/core": "^0.0.5"
  }
}
