  "license": "MIT",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "maintainers": [
    "Vit Bokisch <vit@bokisch.cz>"
  ],
  "homepage": "https://vitus-labs.github.io/vitus-labs/coolgrid/",
  "description": "Ultra flexible and extensible grid system inspired by Bootstrap grid, based on styled-components",
  "main": "lib/vitus-labs-coolgrid.js",
  "umd:main": "lib/vitus-labs-coolgrid.umd.js",
  "module": "lib/vitus-labs-coolgrid.module.js",
  "browser": {
    "./lib/vitus-labs-coolgrid.js": "./lib/vitus-labs-coolgrid.js",
    "./lib/vitus-labs-coolgrid.module.js": "./lib/vitus-labs-coolgrid.module.js"
  },
  "unpkg": "lib/vitus-labs-coolgrid.umd.min.js",
  "jsnext:main": "lib/vitus-labs-coolgrid.module.js",
  "react-native": "lib/vitus-labs-coolgrid.module.js",
  "files": [
    "lib/"
  ],
  "keywords": [
    "vitus-labs",
    "grid",
    "react",
    "styled-components",
    "bootstrap",
    "flexible",
    "extensible",
    "coolgrid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vitus-labs/vitus-labs"
  },
  "bugs": {
    "url": "https://github.com/vitus-labs/vitus-labs/issues"
  },
  "scripts": {
    "dev": "yarn vl_run-stories",
    "prepublish": "yarn build",
    "build": "rm -rf lib && yarn vl_build",
    "build:watch": "yarn vl_build-watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs/storybook",
    "docs:dev": "docz dev",
    "docs:build": "docz build",
    "clean": "rm -rf lib",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint './src/**/*.js*'",
    "lint:js": "eslint src",
    "lint:ts": "tslint -p ./tsconfig.json \"./src/**/*.ts*\"",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "cover": "coveralls < .coverage/lcov.info"
  },
  "peerDependencies": {
    "@vitus-labs/core": "*",
    "@vitus-labs/unistyle": "*",
    "react": ">= 16.7"
  },
  "devDependencies": {
    "@vitus-labs/core": "*",
    "@vitus-labs/tools-babel": "0.1.3",
    "@vitus-labs/tools-rollup": "0.1.2",
    "@vitus-labs/tools-typescript": "0.1.2",
    "@vitus-labs/tools-storybook": "*",
    "@vitus-labs/unistyle": "*"
  }
}
