  "license": "MIT",
  "main": "dist/vitus-labs-elements.js",
  "umd:main": "dist/vitus-labs-elements.umd.js",
  "module": "dist/vitus-labs-elements.module.js",
  "browser": {
    "./dist/vitus-labs-elements.js": "./dist/vitus-labs-elements.js",
    "./dist/vitus-labs-elements.module.js": "./dist/vitus-labs-elements.module.js"
  },
  "unpkg": "dist/vitus-labs-elements.umd.min.js",
  "jsnext:main": "dist/vitus-labs-elements.module.js",
  "react-native": "dist/vitus-labs-elements.module.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf lib dist && babel src -d lib --root-mode upward && cpr lib dist --delete-first && yarn rollup",
    "storybook": "start-storybook -p 6006 -c tools/storybook/web",
    "storybook:build": "build-storybook -c tools/storybook/web -o docs/storybook",
    "docs:dev": "docz dev",
    "docs:build": "docz build",
    "lint": "yarn lint:css && yarn lint:ts",
    "lint:css": "stylelint './src/**/*.ts*'",
    "lint:ts": "tslint -p ./tsconfig.json \"./src/**/*.ts*\"",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "cover": "coveralls < .coverage/lcov.info",
    "rollup": "rollup -c",
    "pkgs": "rm -rf node_modules/ package-lock.json yarn.lock && yarn",
    "prettier": "prettier \"./*.+(js|json|md)\" \"./src/**/*.*\" --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitus-labs/elements"
  },
  "bugs": {
    "url": "https://github.com/vitus-labs/elements/issues"
  },
  "peerDependencies": {
    "@vitus-labs/core": ">= 0.2 <1",
    "react": ">= 16.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.2",
    "@internal/rollup": "*",
    "@storybook/addon-notes": "^5.2.0-beta.33",
    "@storybook/addon-storysource": "^5.2.0-beta.33",
    "@storybook/addon-viewport": "^5.2.0-beta.33",
    "@storybook/addons": "^5.2.0-beta.33",
    "@storybook/react": "^5.2.0-beta.33",
    "@vitus-labs/core": "*",
    "@vitus-labs/rocketstyle": "*",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "coveralls": "^3.0.2",
    "cpr": "^3.0.1",
    "docz": "^2.0.0-rc.52",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.4.2",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.0-beta.1",
    "jsdom": "^15.1.1",
    "prettier": "^1.16.4",
    "react": "^16.10.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.10.1",
    "react-icons-kit": "^1.2.0",
    "styled-components": "^5.0.0-beta.9",
    "typescript": "^3.6.3"
  }
}
