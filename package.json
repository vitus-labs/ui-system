{
  "name": "@vitus-labs/vitus-labs",
  "version": "0.0.0",
  "private": true,
  "description": "vitus-labs",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "license": "MIT",
  "homepage": "https://github.com/vitus-labs/ui-system#readme",
  "directories": {
    "example": "examples"
  },
  "engines": {
    "yarn": ">=1.22"
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "packages/*",
      "tools/*"
    ],
    "nohoist": [
      "**/react-native",
      "**/react-native/**",
      "**/expo",
      "**/expo/**"
    ]
  },
  "scripts": {
    "release": "lerna publish --force-publish",
    "lint": "lerna run lint",
    "pkgs:clean": "rm -rf node_modules/ yarn.lock && yarn",
    "pkgs:build": "lerna run build --stream",
    "prettier": "prettier \"./*.+(js|json|md)\" \"./src/**/*.*\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitus-labs/ui-system.git"
  },
  "bugs": {
    "url": "https://github.com/vitus-labs/ui-system/issues"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@types/enzyme": "^3.10.11",
    "@types/react": "^17.0.38",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-is": "^17.0.3",
    "@types/styled-components": "^5.1.21",
    "@vitus-labs/tools-lint": "^0.25.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "@types/styled-components": "^5.1.22",
    "@vitus-labs/tools-lint": "^0.26.0",
    "coveralls": "^3.1.1",
    "cpr": "^3.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16.3": "^1.7.4",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "react": "^17.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^17.0.1",
    "react-icons-kit": "^2.0.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0"
  }
}
