{
  "name": "@vitus-labs/ui-system-root",
  "version": "0.0.0",
  "private": true,
  "description": "vitus-labs",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/vitus-labs/ui-system#readme",
  "directories": {
    "example": "examples"
  },
  "engines": {
    "yarn": ">=1.22"
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "packages/*"
    ]
  },
  "scripts": {
    "release": "lerna publish --force-publish",
    "lint": "lerna run lint",
    "pkgs:clean": "rm -rf node_modules/ yarn.lock && yarn",
    "pkgs:build": "lerna run build --stream",
    "prettier": "prettier \"./*.+(js|json|md)\" \"./src/**/*.*\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitus-labs/ui-system.git"
  },
  "bugs": {
    "url": "https://github.com/vitus-labs/ui-system/issues"
  },
  "devDependencies": {
    "@types/node": "22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-is": "^19.0.0",
    "@vitus-labs/tools-lint": "^1.4.0",
    "coveralls": "^3.1.1",
    "cpr": "^3.0.1",
    "lerna": "^8.2.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^19.0.0",
    "react-icons-kit": "^2.0.0",
    "styled-components": "^6.1.15",
    "typescript": "5.8.2"
  },
  "packageManager": "yarn@3.2.0"
}
