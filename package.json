{
  "name": "@vitus-labs/vitus-labs",
  "version": "0.0.0",
  "private": true,
  "description": "vitus-labs",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "license": "MIT",
  "homepage": "https://github.com/vitus-labs/ui-system#readme",
  "directories": {
    "example": "examples"
  },
  "engines": {
    "yarn": ">=1.22"
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "packages/*",
      "tools/*"
    ],
    "nohoist": [
      "**/react-native",
      "**/react-native/**",
      "**/expo",
      "**/expo/**"
    ]
  },
  "scripts": {
    "release": "lerna publish --force-publish",
    "lint": "lerna run lint",
    "pkgs:clean": "rm -rf node_modules/ yarn.lock && yarn",
    "pkgs:build": "lerna run build --stream",
    "prettier": "prettier \"./*.+(js|json|md)\" \"./src/**/*.*\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitus-labs/ui-system.git"
  },
  "bugs": {
    "url": "https://github.com/vitus-labs/ui-system/issues"
  },
  "devDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@vitus-labs/tools-lint": "^0.15.0",
    "coveralls": "^3.1.1",
    "cpr": "^3.0.1",
    "lerna": "^4.0.0",
    "prettier": "^2.4.1",
    "react": "^17.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^17.0.1",
    "react-icons-kit": "^2.0.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4"
  }
}
